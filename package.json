{
  "name": "soundbit",
  "version": "1.0.0",
  "private": true,
  "description": "App Description",
  "author": "Almighty42",
  "license": "UNLICENSED",
  "dependencies": {
    "@electron-forge/maker-rpm": "^6.0.0-beta.65",
    "@electron-forge/maker-wix": "^6.0.0-beta.65",
    "@emotion/react": "^11.10.0",
    "@emotion/styled": "^11.10.0",
    "@material-ui/core": "^4.12.3",
    "@material-ui/icons": "^4.11.2",
    "@material-ui/styles": "^4.11.4",
    "@mui/icons-material": "^5.8.4",
    "@mui/material": "^5.10.0",
    "@mui/styles": "^5.9.3",
    "@reach/slider": "^0.15.3",
    "@testing-library/jest-dom": "^5.14.1",
    "@testing-library/react": "^11.2.7",
    "@testing-library/user-event": "^12.8.3",
    "clsx": "^1.1.1",
    "concurrently": "^6.2.0",
    "d3": "^7.0.0",
    "electron-hot-reload": "^0.1.4",
    "electron-installer-dmg": "^4.0.0",
    "electron-reload": "^2.0.0-alpha.1",
    "electron-squirrel-startup": "^1.0.0",
    "framer-motion": "^4.1.17",
    "jsmediatags": "^3.9.7",
    "mp3tag": "^1.0.3",
    "music-metadata": "^7.11.4",
    "music-metadata-browser": "^2.5.0",
    "musicmetadata": "^2.0.5",
    "react": "^17.0.2",
    "react-audio-player": "^0.17.0",
    "react-dom": "^17.0.2",
    "react-lodash": "^0.1.2",
    "react-router-dom": "^5.2.0",
    "react-scripts": "4.0.3",
    "read-audio-tags": "^0.1.1",
    "wait-on": "^6.0.0",
    "web-vitals": "^1.1.2"
  },
  "main": "main.js",
  "build": {
    "appId": "Soundbit",
    "extends": null,
    "win": {
      "target": [
        "nsis"
      ],
      "icon": "src/SoundBitIcon.ico"
    },
    "nsis": {
      "installerIcon": "src/SoundBitIcon.ico",
      "uninstallerIcon": "src/SoundBitIcon.ico",
      "oneClick": false,
      "allowToChangeInstallationDirectory": true
    },
    "files": [
      "*.js",
      "build",
      "node_modules"
    ]
  },
  "scripts": {
    "start": "electron-forge start",
    "electron": "electron .",
    "electron-buildrun": "react-scripts build && electron .",
    "package": "electron-forge package",
    "build-installer": "electron-builder",
    "make": "electron-forge make"
  },
  "homepage": ".",
  "eslintConfig": {
    "extends": [
      "react-app",
      "react-app/jest"
    ]
  },
  "browserslist": {
    "production": [
      ">0.2%",
      "not dead",
      "not op_mini all"
    ],
    "development": [
      "last 1 chrome version",
      "last 1 firefox version",
      "last 1 safari version"
    ]
  },
  "devDependencies": {
    "@electron-forge/cli": "^6.0.0-beta.61",
    "@electron-forge/maker-deb": "^6.0.0-beta.61",
    "@electron-forge/maker-dmg": "^6.0.0-beta.65",
    "@electron-forge/maker-squirrel": "^6.0.0-beta.61",
    "electron": "^20.0.1",
    "electron-builder": "^23.3.3",
    "npm-watch": "^0.11.0"
  },
  "config": {
    "forge": {
      "electronRebuildConfig": {
        "forceABI": 89
      },
      "packagerConfig": {
        "icon": "./src/SoundBitIcon"
      },
      "makers": [
        {
          "name": "@electron-forge/maker-deb",
          "config": {
            "options": {
              "icon": "./src/SoundBitIcon.png"
            }
          }
        },
        {
          "name": "@electron-forge/maker-dmg",
          "platforms": [
            "darwin"
          ],
          "config": {
            "format": "ULFO"
          }
        }
      ]
    }
  }
}
